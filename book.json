{
  "title": "iOS开发手册",
  "description": "iOS开发手册",
  "author": "iOS开发手册小组",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.2",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "-livereload",
    "search-plus@^0.0.11",
    "splitter@^0.0.8",
    "edit-link@^2.0.2",
    "tbfed-pagefooter@^0.0.1", 
    "anchor-navigation-ex@0.1.8",
    "github-buttons@2.1.0",
    "ace@^0.3.2",
    "toc2"
  ],
  "pluginsConfig": {
	"sharing": {
            "weibo": true,
            "facebook": false,
            "twitter": false,
            "google": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper"
            ]
        },
    "github": {
      "url": "https://github.com/PanZhow/iOS_notebook"
    },
	"tbfed-pagefooter": {
            "copyright": "Copyright © iOS开发手册小组 ",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
	"anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "isShowTocTitleIcon": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
    "edit-link": {
      "base": "https://github.com/PanZhow/iOS_notebook/edit/master",
      "label": "编辑本页"
    },
 	"github-buttons": {
            "repo": "PanZhow/iOS_notebook",
            "types": [
                "star"
            ],
            "size": "small"
        },
         "toc2": {
            "addClass": true,
            "className": "toc"
        },
    "sitemap": {
        "hostname": "http://www.jianshu.com/"
    }
  }
}